package lab_2;
import java.util.ArrayList;
import java.util.Scanner;

public class task_2 {
    public static void main(String[] args) {
        Scanner myObj = new Scanner(System.in);
        ArrayList<String> todoList = new ArrayList<>();
        todoList.add("Buy groceries");
        todoList.add("Iron clothes");
        todoList.add("Clean room");
        todoList.add("Call Ammi");
        todoList.add("Prepare Dinner");

        System.out.println("To do List: ");
        for (String n : todoList){
            System.out.println(n);
        }
        todoList.remove("Iron clothes");
        System.out.println("");
        System.out.println("Updated list after removing 'Iron clothes'");
        for (String n : todoList){
            System.out.println(n);
        }
    }
}
